{
  "name": "nmt",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Faouzi Mohamed",
  "scripts": {
    "format": "prettier  --write './**/*.{jsx,js,css,json}'",
    "lint": "eslint ./ --ext js,jsx --fix",
    "csslint": "stylelint ./styles --fix",
    "stylelint-check": "stylelint-config-prettier-check",
